# Default is to keep crlf line endings
text=auto

README.md text eol=crlf
*.DOC text eof=crlf
*.AS text eol=crlf linguist-language=Assembly
*.BIN binary
*.C text eol=crlf linguist-language=C
*.COM binary
*.H text eol=crlf linguist-language=C
*.MAC text eol=crlf linguist-language=Assembly
*.SUB text eol=crlf
*.HEX text eol=crlf
*.lbr binary
*.LBR binary
*.OBJ binary
*.RUN binary
*.tar binary
*.TAR binary
*.zip binary
*.ZIP binary
*.SYS binary
*.jpg binary
*.pdf binary
*.HLP binary
*.gz binary
*.PLM text eol=crlf
*.DAT text eol=crlf
diskdef* text eol=crlf
